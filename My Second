public class Yearsanddays{
     public static void main(String []args){
        System.out.println("Hello World");
        int a=3456789;
        int b,c,d;
        b=a/(365*24*60);
        c=a%(365*24*60);
        d=c/(1440);
       System.out.println(+b+" years and "+d+" days");
        
     }
}
